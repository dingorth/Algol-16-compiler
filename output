prog
(
	Piec_Dziesiec_Pietnascie,
	block
	(
		[
			procedure
			(
				f,
				[x],
				block
				(
					[	
						procedure
						(
							one,
							[],
							block
							(
								[],
								[write(const(5)),return(const(1))]
							)
						)
					],
					[
						if(
							gt(var(x),const(0)),
							[call(procedure(f,[minus(var(x),procedure(one,[]))]))]
						)
					]
				)
			)
		],
		[call(procedure(f,[const(5)]))]
	)
)